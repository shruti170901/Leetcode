class Solution {
public:
    int subtractProductAndSum(int n) {
        int sum, product, result;
        sum=0;
        product=1;
        
       
        while(n){
            sum=sum+(n%10);
            product=product*(n%10);
            n=n/10;
            
        }
        return product-sum;
        
    return 0; }
};
